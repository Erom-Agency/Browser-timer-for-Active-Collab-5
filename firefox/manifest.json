{
	"manifest_version": 2,
	"name": "Browser timer for Active Collab 5",
	"short_name": "Browser timer for AC 5",
	"description": "Web timer for Active Collab. It stores time for each task in the time tracking section so you can use it for billing and reports.",
	"version": "1.0.2",
	"icons": {"16": "icons/icon.png", "32":"icons/icon.png", "48":"icons/icon.png", "128":"icons/icon.png"},
	"author": "Erom Agency",
	"homepage_url": "http://eromagency.com/actimer/",
	"content_scripts": [{
		"matches": ["*://*/*"],
		"js": ["data/inc/jquery.min.js", "data/inc/jquery-ui.min.js", "data/inc/main.js", "data/bootstrap.js"],
		"css": ["data/inc/main.css"]
	}],
	
	"options_ui": {
		"page": "options/options.html"
	},
	"permissions": [
		"storage"
	]
}